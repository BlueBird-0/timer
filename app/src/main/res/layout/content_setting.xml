<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_setting">
    <ImageView
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:background="@drawable/shape_setting_box"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" android:id="@+id/imageView3"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사운드"
            android:textSize="25dp"
            android:textStyle="bold"
            android:textColor="@color/colorOrange"
            app:layout_constraintStart_toStartOf="@+id/imageView3" android:layout_marginStart="15dp"
            app:layout_constraintTop_toTopOf="@+id/imageView3"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" android:id="@+id/textView4"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:background="@drawable/shape_setting_box"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView4"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="우주"
            android:textSize="25dp"
            android:textColor="@color/colorOrange"
            app:layout_constraintTop_toTopOf="@+id/imageView4"
            app:layout_constraintStart_toStartOf="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            android:layout_marginStart="32dp" android:id="@+id/textView5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:background="@drawable/shape_setting_box" app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:id="@+id/imageView5"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="백색소음"
            android:textSize="25dp"
            android:textColor="@color/colorOrange" app:layout_constraintTop_toTopOf="@+id/imageView5"
            app:layout_constraintStart_toStartOf="@+id/imageView5" android:layout_marginStart="32dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView5"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_checked" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:background="@drawable/shape_setting_box"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:id="@+id/imageView7"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_speaker"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7" app:layout_constraintStart_toStartOf="@+id/imageView7"
            android:layout_marginStart="32dp" android:id="@+id/imageView8"/>
    <SeekBar
            android:id="@+id/seekBar1"
            android:layout_width="299dp"
            android:layout_height="19dp"
            android:outlineAmbientShadowColor="@color/colorOrange"
            android:outlineSpotShadowColor="@color/colorOrange"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:layout_constraintStart_toEndOf="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"/>





</android.support.constraint.ConstraintLayout>